version    2.0

config setup
    dumpdir=/var/run/pluto/
    nat_traversal=yes
    oe=off
    protostack=netkey

conn link
    type=tunnel
    authby=secret

    left={{ my_private }}
    leftsubnet={{ my_subnet }}
    leftnexthop=%defaultroute

    right={{ right_ip }}
    rightsourceip={{ other_private }}
    rightsubnet={{ other_subnet }}
    rightnexthop=%defaultroute

    aggrmode=no
    auto=start
